{
  "name": "fyle-partner-dashboard-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run setup:env && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "unit_test:ci": "npm run setup:env && ng test --watch=false --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "generate:svg-sprite": "svg2sprite ./src/assets/icons ./src/assets/sprites/sprite.svg --stripAttrs fill --stripAttrs stroke --stripAttrs id",
    "setup:env": "node scripts/setup_env.js",
    "cypress:open": "cypress open",
    "stop": "kill $(lsof -t -i:4200) || true"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^18.2.14",
    "@angular/animations": "^18.2.13",
    "@angular/cli": "^18.2.14",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@auth0/angular-jwt": "^5.1.0",
    "@sentry/angular": "^8.18.0",
    "@sentry/cli": "^2.32.2",
    "eslint": "^8.57.0",
    "ng-svg-icon-sprite": "^1.14.0",
    "primeicons": "^7.0.0",
    "primeng": "^17.17.0",
    "rxjs": "^7.8.1",
    "tailwindcss": "~3.2.1",
    "ts-cacheable": "^1.0.8",
    "tslib": "^2.3.0",
    "typescript": "~5.4.5",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "^18.4.3",
    "@angular-eslint/template-parser": "14.1.2",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "autoprefixer": "^10.4.13",
    "esbuild": "^0.25.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.18",
    "svg2sprite-cli": "^2.0.1"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "trim-newlines": "^4.0.2",
    "json5": "^2.2.2",
    "vite": "5.4.12",
    "karma": {
      "minimatch": "^3.0.5"
    }
  }
}
