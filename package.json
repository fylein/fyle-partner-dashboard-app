{
  "name": "fyle-partner-dashboard-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run setup:env && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "unit_test:ci": "npm run setup:env && ng test --watch=false --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "generate:svg-sprite": "svg2sprite ./src/assets/icons ./src/assets/sprites/sprite.svg --stripAttrs fill --stripAttrs stroke --stripAttrs id",
    "setup:env": "node scripts/setup_env.js",
    "cypress:open": "cypress open",
    "stop": "kill $(lsof -t -i:4200) || true"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular/animations": "^14.2.0",
    "@angular/cli": "~14.2.2",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@sentry/angular": "^7.41.0",
    "@sentry/tracing": "^7.41.0",
    "ng-svg-icon-sprite": "^1.12.0",
    "primeicons": "6.0.1",
    "primeng": "^14.2.2",
    "rxjs": "~6.6.0",
    "tailwindcss": "3.1.8",
    "ts-cacheable": "1.0.6",
    "tslib": "^2.2.0",
    "typescript": "~4.7.2",
    "webpack": "^5.75.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "14.0.4",
    "@angular-eslint/eslint-plugin-template": "14.0.4",
    "@angular-eslint/schematics": "^1.2.0",
    "@angular-eslint/template-parser": "14.0.4",
    "@cypress/code-coverage": "^1.14.0",
    "@types/jasmine": "~3.8.0",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "autoprefixer": "^10.4.12",
    "cypress": "10.3.0",
    "eslint": "^7.26.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.16",
    "svg2sprite-cli": "^2.0.1"
  },
  "nyc": {
    "report-dir": "cypress/coverage",
    "reporter": [
      "text-summary",
      "html",
      "json-summary"
    ]
  }
}
